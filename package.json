{
  "name": "vdanilov",
  "version": "1.0.0",
  "description": "Personal website of Viacheslav Danilov",
  "scripts": {
  "prepare": "husky && node --eval \"fs.writeFileSync('.husky/pre-commit','npx lint-staged\\n')\"",
    "format": "prettier --write .",
    "check": "prettier --check ."
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,css,scss,md,html}": [
      "prettier --write"
    ]
  },
  "devDependencies": {
    "husky": "9.1.7",
    "lint-staged": "16.2.4",
    "prettier": "3.6.2"
  }
}
